# demo script to be assembled with asb
# Arthur Jacquin (arthur@jacquin.xyz) - January 2024

jmp main

fibo:
    # x2: n
    mov x6, 1
    mov x7, 1
    inc x2
fibo_loop:
    dec x2
    mov x8, x7
    add x7, x7, x6
    mov x6, x8
    bgt x2, 0, fibo_loop
    mov x1, x6
    ret

main:
    mov x2, 12
    call fibo

end:
    jmp end
